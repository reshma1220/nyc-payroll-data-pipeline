{
	"name": "dataflow_agency",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_csv_agency",
						"type": "DatasetReference"
					},
					"name": "srcagency"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_agency",
						"type": "DatasetReference"
					},
					"name": "sinkagency"
				}
			],
			"transformations": [],
			"scriptLines": [
				"parameters{",
				"     parameter1 as integer (2020)",
				"}",
				"source(output(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> srcagency",
				"srcagency sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sinkagency"
			]
		}
	}
}